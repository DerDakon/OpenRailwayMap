<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0">
	<group name="OpenRailwayMap" icon="signals.png">
		<item name="Gleis" icon="rail.png" type="way">
			<key key="railway" value="rail" />
			<text key="ref" text="Track ref" de.text="Streckennummer" default="" delete_if_empty="true" />
			<text key="name" text="Railway name" de.text="Streckenname" default="" delete_if_empty="true" />
			<combo key="usage" text="Usage" de.text="Nutzung" values="main,branch,highspeed,industrial,military,tourism,test" display_values="Hauptstrecke,Nebenstrecke,Hochgeschwindigkeitsstrecke,Industriebahn,Militärbahn,Museumsbahn,Teststrecke" default="" delete_if_empty="true" />
			<combo key="electrified" text="Electrification" de.text="Elektrifizierung" values="yes,no,contact_line,rail" display_values="Ja,Nein,Oberleitung,Stromschiene" default="" delete_if_empty="true" />
			<combo key="railway:traffic_mode" text="Traffic mode" de.text="Verkehrsart" values="mixed,passenger,freight" display_values="Gemischt,Personenverkehr,Güterverkehr" default="" delete_if_empty="true" />
			<check key="lit" text="Lit" de.text="Beleuchtung" default="off" delete_if_empty="true" />
			<check key="railway:pzb" text="PZB" de.text="PZB" default="off" delete_if_empty="true" />
			<check key="railway:lzb" text="LZB" de.text="LZB" default="off" delete_if_empty="true" />
		</item>
		<group name="Signale" icon="signals.png">
			<item name="Ks-Mehrabschnitssignal" icon="ks.png" type="node">
				<key key="railway" value="signal" />
				<text key="ref" text="Signal ref" de.text="Signalnummer" default="" delete_if_empty="true" />
				<key key="railway:signal:combined" value="ks" />
				<key key="railway:signal:combined:form" value="light" />
				<combo key="railway:signal:combined:height" text="Signal height" de.text="Signalbauform" values="normal,dwarf" display_values="Mastbauform,Zwergsignal" default="" delete_if_empty="true" />
				<key key="railway:signal:combined:states" value="hp0;ks1;ks2" />
				<combo key="railway:signal:combined:function" text="Signal function" de.text="Signalaufgabe" values="in,out,block,between" display_values="Einfahrsignal,Ausfahrsignal,Blocksignal,Zwischensignal" default="" delete_if_empty="true" />
				<multiselect key="railway:signal:combined:substitute_signal" text="Substitue signal (Multiselection)" de.text="Ersatzsignal (Mehrfachauswahl)" delimiter=";" values="zs1;zs7;zs12" display_values="Ersatzsignal;Vorsichtsignal;M-Tafel" default="" delete_if_empty="true" />
				<check key="railway:signal_bridge" text="Signal bridge" de.text="Signalbrücke" default="off" delete_if_empty="true" />
			</item>
			<item name="Ks-Vorsignal" icon="ks-distant.png" type="node">
				<key key="railway" value="signal" />
				<text key="ref" text="Signal ref" de.text="Signalnummer" default="" delete_if_empty="true" />
				<key key="railway:signal:distant" value="ks" />
				<key key="railway:signal:distant:form" value="light" />
				<combo key="railway:signal:distant:height" text="Signal height" de.text="Signalbauform" values="normal,dwarf" display_values="Mastbauform,Zwergsignal" default="" delete_if_empty="true" />
				<key key="railway:signal:distant:states" value="ks1;ks2" />
				<check key="railway:signal:distant:shortened" text="Shortened distance" de.text="Verkürzter Bremsweg" default="off" delete_if_empty="true" />
				<check key="railway:signal:distant:repeated" text="Repeated Signal" de.text="Vorsignalwiederholer" default="off" delete_if_empty="true" />
				<check key="railway:signal:distant:marker_light" text="Marker light" de.text="Kennlicht" default="off" delete_if_empty="true" />
				<combo key="railway:signal:position" text="Signal position" de.text="Signalstandort" values="left,right,bridge" display_values="Links von Wegrichtung,Rechts von Wegrichtung,Signalbrücke" default="" delete_if_empty="true" />
				<combo key="railway:signal:direction" text="Direction" de.text="Anzeigerichtung" values="forward,backward" display_values="In Wegrichtung,Entgegen Wegrichtung" default="" delete_if_empty="true" />
			</item>
			<item name="Ks-Hauptsignal" icon="ks-main.png" type="node">
				<key key="railway" value="signal" />
				<text key="ref" text="Signal ref" de.text="Signalnummer" default="" delete_if_empty="true" />
				<key key="railway:signal:main" value="ks" />
				<key key="railway:signal:main:form" value="light" />
				<combo key="railway:signal:main:height" text="Signal height" de.text="Signalbauform" values="normal,dwarf" display_values="Mastbauform,Zwergsignal" default="" delete_if_empty="true" />
				<key key="railway:signal:main:states" value="hp0;ks1" />
				<check key="railway:signal_bridge" text="Signal bridge" de.text="Signalbrücke" default="off" delete_if_empty="true" />
			</item>
				<item name="Hp-Hauptsignal" icon="hp.png" type="node">
				<key key="railway" value="signal" />
				<text key="ref" text="Signal ref" de.text="Signalnummer" default="" delete_if_empty="true" />
				<key key="railway:signal:main" value="hp" />
				<combo key="railway:signal:main:form" text="Signal form" de.text="Anzeigeart" values="light,semaphore" display_values="Lichtsignal,Formsignal" default="" delete_if_empty="true" />
				<combo key="railway:signal:main:height" text="Signal height" de.text="Signalbauform" values="normal,dwarf" display_values="Mastbauform,Zwergsignal" default="" delete_if_empty="true" />
				<multiselect key="railway:signal:main:states" text="Signal states (Multiselection)" de.text="Signalzustände (Mehrfachauswahl)" delimiter=";" values="hp0;hp1;hpvr2" display_values="Halt;Fahrt;Langsamfahrt" default="" delete_if_empty="true" />
				<combo key="railway:signal:main:function" text="Signal function" de.text="Signalaufgabe" values="in,out,block,between" display_values="Einfahrsignal,Ausfahrsignal,Blocksignal,Zwischensignal" default="" delete_if_empty="true" />
				<multiselect key="railway:signal:main:substitute_signal" text="Substitue signal (Multiselection)" de.text="Ersatzsignal (Mehrfachauswahl)" delimiter=";" values="zs1;zs7;zs12" display_values="Ersatzsignal;Vorsichtsignal;M-Tafel" default="" delete_if_empty="true" />
				<check key="railway:signal_bridge" text="Signal bridge" de.text="Signalbrücke" default="off" delete_if_empty="true" />
			</item>
			<item name="Vr-Vorsignal" icon="vr.png" type="node">
				<key key="railway" value="signal" />
				<text key="ref" text="Signal ref" de.text="Signalnummer" default="" delete_if_empty="true" />
				<key key="railway:signal:advance" value="ks" />
				<combo key="railway:signal:advance:form" text="Signal form" de.text="Anzeigeart" values="light,semaphore" display_values="Lichtsignal,Formsignal" default="" delete_if_empty="true" />
				<combo key="railway:signal:advance:height" text="Signal height" de.text="Signalbauform" values="normal,dwarf" display_values="Mastbauform,Zwergsignal" default="" delete_if_empty="true" />
				<multiselect key="railway:signal:advance:states" text="Signal states (Multiselection)" de.text="Signalzustände (Mehrfachauswahl)" delimiter=";" values="vr0;vr1;vr2" display_values="Halt erwarten;Fahrt erwarten;Langsamfahrt erwarten" default="" delete_if_empty="true" />
				<check key="railway:signal:advance:shortened" text="Shortened distance" de.text="Verkürzter Bremsweg" default="off" delete_if_empty="true" />
				<check key="railway:signal:advance:repeated" text="Repeated Signal" de.text="Vorsignalwiederholer" default="off" delete_if_empty="true" />
				<check key="railway:signal_bridge" text="Signal bridge" de.text="Signalbrücke" default="off" delete_if_empty="true" />
			</item>
			<item name="Pfeiftafel" icon="de:whistle.png" type="node">
				<key key="railway" value="signal" />
				<text key="railway:signal:whistle" text="Exact type (e.g. 'bü4')" de.text="Signaltyp (z.B. 'bü4')" default="yes" delete_if_empty="true" />
				<key key="railway:signal:whistle:form" value="sign" />
				<check key="railway:signal:whistle:twice" text="Whistle twice?" de.text="Doppelte Pfeiftafel?" default="off" delete_if_empty="true" />
				<check key="railway:signal:whistle:only_transit" text="Whistle only when train does not stop before crossing?" de.text="Zwei senkrechte Striche?" default="off" delete_if_empty="true" />
				<check key="railway:signal_bridge" text="Signal bridge" de.text="Signalbrücke" default="off" delete_if_empty="true" />
			</item>
			<item name="Läutetafel" icon="de:ring.png" type="node">
				<key key="railway" value="signal" />
				<key key="railway:signal:whistle" value="bü5" />
				<key key="railway:signal:whistle:form" value="sign" />
			</item>
		</group>
		<item name="Kilometerstein" icon="milestone.png" type="node">

		</item>
		<item name="Weiche" icon="switch.png" type="node">
			<key key="railway" value="switch" />
			<text key="ref" text="Switch ref" de.text="Weichennummer" default="" delete_if_empty="true" />
			<check key="railway:manual" text="Manual" de.text="Handgestellt" default="off" delete_if_empty="true" />
			<combo key="railway:switch" text="Switch type" de.text="Weichentyp" values="default,three_way,single_slip,double_slip,backfall,wye" display_values="Standardweiche,Dreiwegweiche,Kreuzungsweiche,Doppelkreuzungsweiche,Rückfallweiche,Y-Weiche" default="" delete_if_empty="true" />
		</item>
		<item name="Bahnhof/Haltepunkt" icon="station.png" type="node">
			<combo key="railway" text="Type" de.text="Typ" values="station,halt" display_values="Bahnhof,Haltepunkt" default="" delete_if_empty="true" />
			<text key="name" text="Name" de.text="Name" default="" delete_if_empty="true" />
			<text key="railway:short" text="Short name" de.text="Abkürzung" default="" delete_if_empty="true" />
			<text key="uic_ref" text="UIC-ID" de.text="UIC-Nummer" default="" delete_if_empty="true" />
			<check key="on_demand" text="On demand" de.text="Bedarfshalt" default="off" delete_if_empty="true" />
		</item>
		<item name="Rangierbahnhof" icon="yard.png" type="node,closedway">
			<key key="railway" value="yard" />
			<text key="name" text="Name" de.text="Name" default="" delete_if_empty="true" />
			<text key="railway:short" text="Short name" de.text="Abkürzung" default="" delete_if_empty="true" />
		</item>
		<item name="Stellwerk" icon="signalbox.png" type="node,closedway">
			<key key="railway" value="signal_box" />
			<text key="name" text="Name" de.text="Name" default="" delete_if_empty="true" />
			<text key="railway:short" text="Short name" de.text="Abkürzung" default="" delete_if_empty="true" />
		</item>
		<item name="Abzweig" icon="junction.png" type="node">
			<key key="railway" value="junction" />
			<text key="name" text="Name" de.text="Name" default="" delete_if_empty="true" />
			<text key="railway:short" text="Short name" de.text="Abkürzung" default="" delete_if_empty="true" />
			<text key="ref" text="Switch ref" de.text="Weichennummer" default="" delete_if_empty="true" />
			<check key="railway:manual" text="Manual" de.text="Handgestellt" default="off" delete_if_empty="true" />
			<combo key="railway:switch" text="Switch type" de.text="Weichentyp" values="default,three_way,single_slip,double_slip,backfall,wye" display_values="Standardweiche,Dreiwegweiche,Kreuzungsweiche,Doppelkreuzungsweiche,Rückfallweiche,Y-Weiche" default="" delete_if_empty="true" />
		</item>
		<item name="Bahnübergang" icon="crossing.png" type="node">
			<combo key="railway" text="Type" de.text="Typ" values="level_crossing,crossing" display_values="Bahnübergang,Fußübergang" default="" delete_if_empty="true" />
			<text key="name" text="Name (no position)" de.text="Name (keine Kilometerangabe)" default="" delete_if_empty="true" />
			<text key="railway:position" text="Position (rounded, e.g. '12.3')" de.text="Kilometerangabe (gerundet, z.B. '12.3')" default="" delete_if_empty="true" />
			<text key="railway:position" text="Position (exact, e.g. '12.345')" de.text="Kilometerangabe (genau, z.B: '12.345')" default="" delete_if_empty="true" />
			<check key="crossing:light" text="Lights" de.text="Warnlicht" default="off" delete_if_empty="true" />
			<check key="crossing:on_demand" text="On demand" de.text="Anrufschranke" default="off" delete_if_empty="true" />
			<check key="crossing:bell" text="Bell" de.text="Akustischer Warnton" default="off" delete_if_empty="true" />
			<check key="crossing:chicane" text="Chicane" de.text="Umlaufgitter" default="off" delete_if_empty="true" />
			<combo key="crossing:barrier" text="Barriers" de.text="Schranken" values="half,double_half,full,yes,no" display_values="Halbschranke,Doppelte Halbschranke,Vollschranke,Beschrankt,Unbeschrankt" default="" delete_if_empty="true" />
		</item>
		<item name="Fernsprecher" icon="phone.png" type="node">
			<key key="railway" value="phone" />
		</item>
		<item name="Stromsystemwechsel" icon="systemswitch.png" type="node">
			<key key="railway" value="isolated_track_section" />
		</item>
		<item name="Zugfunk" icon="radio.png" type="node">
			<key key="railway" value="radio" />
			<key key="man_made" value="tower" />
			<key key="tower:type" value="communication" />
			<combo key="railway:radio" text="Technik" de.text="Mode" values="analogue,gsm-r" display_values="Analog,GSM-R" default="" delete_if_empty="true" />
		</item>
		<item name="Wasserturm" icon="watertower.png" type="node,closedway">
			<key key="railway" value="water_tower" />
			<key key="man_made" value="water_tower" />
		</item>
		<item name="Tankstelle" icon="fuel.png" type="node,closedway">
			<key key="railway" value="fuel" />
		</item>
		<item name="Bahngelände" icon="landuse.png" type="closedway">
			<key key="landuse" value="railway" />
		</item>
		<item name="Drehscheibe" icon="turntable.png" type="node,closedway">
			<key key="railway" value="turntable" />
		</item>
		<item name="Schiebebühne" icon="traverser.png" type="node,closedway">
			<key key="railway" value="traverser" />
		</item>
		<item name="Gleiswaage" icon="scale.png" type="node,closedway">
			<key key="railway" value="track_scale" />
		</item>
		<item name="Prellbock" icon="bufferstop.png" type="node">
			<key key="railway" value="buffer_stop" />
		</item>
		<item name="Eisenbahnmuseum" icon="museum.png" type="node,closedway">
			<key key="railway" value="museum" />
			<key key="tourism" value="museum" />
			<text key="name" text="Name" de.text="Name" default="" delete_if_empty="true" />
		</item>
	</group>
</presets>